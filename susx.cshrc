source /local/global/cshrc
source /local/global/cshrc.useful

source $HOME/.dirmanip
source $HOME/.alias

set prompt = "%m %h %%"
unset autologout
set savehist = 100
set history = 100
set nobeep

setenv OPENWINHOME /local/Xol
#setenv XAPPLRESDIR /usr/local/X11R5/pd/lib/X11/app-defaults:$XAPPLRESDIR
setenv poplib $HOME/pop
setenv MYMAIL $HOME/mail/mbox
setenv MBOX $HOME/mail/mbox
setenv MORE -cs

setenv NNTPSERVER syma



